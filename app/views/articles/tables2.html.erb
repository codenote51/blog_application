<table>
  <thead>
    <tr>
    </tr>
  </thead>
  <tbody>
    <% (1..(@n)).to_a.each do |i| %>
      <tr>
        <% (1..params[:m].to_i).to_a.each do |j| %>
          <td><%= i * j %></td>
        <% end %>
      </tr>
        <%= puts "n + m" %>
        <%= puts "10, 19" %>

    <% end %>
  </tbody>
</table>
