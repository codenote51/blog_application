<div class="divclass">
  <%= form_for :session, url: 'login_path' do |form| %>
    <div>
      <%= form.label :email %><br>
      <%= form.text_field :email %><br>
    </div>

    <div>
      <%= form.label :password %><br>
      <%= form.password_field :password %><br><br>
    </div>

    <div>
      <%= form.submit 'login', class: 'btn' %>
    </div>

     <p>New user <%= link_to "Sign up now!", signup_path %></p>

  <% end %>
</div>
